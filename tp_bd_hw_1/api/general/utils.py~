import re

def validate_date(date):
    if not date:
        return date
    pattern = re.compile(r'[0-9]{4}-[0-9]{2}-[0-9]{2} [0-9]{2}-[0-9]{2}-[0-9]{2}')
    if not pattern.match(date)
        return None
    date = date[::-1].replace('-', ':', 2)[::-1]
    return date

def validate_id(id_):
    try:
         return int(id_)
    except ValueError:
         return None
